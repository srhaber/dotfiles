'fileTypes': [
  'gitattributes'
  '.config/git/ignore'
  '.git/info/exclude'
  'gitignore'
  'conf'
  'ctags'
  'hgignore'
  'mailmap'
  'stylelintignore'
  'cfg'
]
'name': 'Generic Configuration'
'patterns': [
  {
    'include': '#comment'
  }
]
'repository':
  'comment':
    'captures':
      '1':
        'name': 'comment.line.number-sign.generic-config'
      '2':
        'name': 'punctuation.definition.comment.generic-config'
      '3':
        'name': 'comment.line.semi-colon.generic-config'
      '4':
        'name': 'punctuation.definition.comment.generic-config'
    'match': '((#).*$\\n?)|((;).*$\\n?)'
'scopeName': 'text.generic-config'
